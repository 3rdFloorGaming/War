// War.cpp : Defines the entry point for the console application.
//

#include <iostream>
#include <vector>
#include <string>
#include "Card.cpp"
#include "Card.h"

using std::vector;
using std::cout;
using std::endl;


//THESE WILL GO IN A SEPARATE HEADER FILE


int main()
{
	//Creates deck that players will use
	vector<Card> deck;

	//For loop that populates deck
    for (int j=0; j<13; j++) 
	{
        for (int i=0; i<4; i++) 
		{
			deck.push_back(Card(Value(j), Suit(i)));
        }               
    }

	//Reads through deck
	for(int i = 0; i < deck.size(); i++)
	{
		cout << deck[i].name() << endl;
	}

	return 0;
}

